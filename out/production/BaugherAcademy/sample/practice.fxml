<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Polygon?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" stylesheets="@css/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.PracticeController">
   <children>
      <Polygon fill="DODGERBLUE" layoutX="64.0" layoutY="75.0" points="-64.0, 646.0, 65.0, 522.0, -64.0, -74.0" stroke="BLACK" strokeType="INSIDE" styleClass="cool_triangle" />
      <Polygon fill="DODGERBLUE" layoutX="315.0" layoutY="489.0" points="-315.0, 232.0, 965.0, 232.0, -186.0, 108.0" stroke="BLACK" strokeType="INSIDE" styleClass="cool_triangle_jr" />
      <ImageView fx:id="questionAnswerImage" fitHeight="500.0" fitWidth="1180.0" layoutX="50.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="viewAnswer" layoutX="497.0" layoutY="548.0" mnemonicParsing="false" onAction="#getAnswer" text="View Answer" />
      <Button fx:id="complete" layoutX="1011.0" layoutY="548.0" mnemonicParsing="false" onAction="#questionComplete" text="Mark as Complete">
         <styleClass>
            <String fx:value="button" />
            <String fx:value="correct_button" />
         </styleClass></Button>
      <Button fx:id="next" layoutX="1011.0" layoutY="620.0" mnemonicParsing="false" onAction="#toNextQuestion" prefHeight="49.0" prefWidth="234.0" text="Next Question" />
      <Button fx:id="viewQuestion" layoutX="254.0" layoutY="548.0" mnemonicParsing="false" onAction="#displayQuestion" text="View Question" />
      <Button fx:id="grading" layoutX="713.0" layoutY="548.0" mnemonicParsing="false" onAction="#displayGradingInstructions" text="Grading Instructions" />
      <Button layoutX="20.0" layoutY="20.0" mnemonicParsing="false" onAction="#goBack" styleClass="back_button" text="←" />
   </children>
</AnchorPane>
